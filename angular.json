{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
        "app": {
            "projectType": "application",
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "tsConfig": "tsconfig.json",
                        "outputPath": {
                            "base": "dist",
                            "browser": "."
                        },
                        "assets": [
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.css"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "options": {
                        "port": 3000
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "app:build:production"
                        },
                        "development": {
                            "buildTarget": "app:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    },
    "cli": {
      "analytics": "4fb2b8c4-8b4c-497b-a001-b9fd81ea8dd7"
    }
}